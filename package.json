    "@angular/cli": "20.3.2",
    "@angular/compiler-cli": "20.3.2",
    "@eslint/js": "^9.36.0",
    "@spartan-ng/cli": "^0.0.1-alpha.597",
    "@types/jasmine": "~5.1.0",
    "angular-eslint": "^20.3.0",
    "eslint": "^9.36.0",
    "eslint-config-prettier": "^10.1.8",
